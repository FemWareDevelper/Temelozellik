local Tab = Window:CreateTab("Teleports", 4483362458) -- Title, Image

local TweenService = game:GetService("TweenService")
local Players = game:GetService("Players")
local localPlayer = Players.LocalPlayer

local pos = Vector3.new(494.55, 16.37, -347.01)
local targetCFrame = CFrame.new(pos)

local function smoothTeleport()
    local character = localPlayer.Character
    if not character then return end
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then return end

    local tweenInfo = TweenInfo.new(
        5,
        Enum.EasingStyle.Quad,
        Enum.EasingDirection.Out 
    )

    local tween = TweenService:Create(humanoidRootPart, tweenInfo, {CFrame = targetCFrame})
    tween:Play()

    tween.Completed:Wait()
    print("Teleport tamamlandı!")
end

local Button = Tab:CreateButton({
    Name = "Test",
    Callback = smoothTeleport
})

